#include <iostream>
#include <cstdio>
#include <cstring>
#include <queue>
using namespace std;

char s[10010][100];
char c[100];
int cnt;

int check(char *s, char *c) {
	if(strcmp(s, c) == 0) return 0;
	int l1 = strlen(s), l2 = strlen(c);
	if(l1 < l2) {
		swap(s, c);
		swap(l1, l2);
	}
	int J = 0;
	for(int i = 0; i < l1; i++) {
		if(J < l2 && c[J] == s[i]) J++;
	}
	if(l1 - J <= 1) return 1;
	return -1;
}

int main() {
	cnt = 0;
	while(true) {
		scanf("%s", s[cnt]);
		if(s[cnt][0] == '#') break;
		else cnt++;
	}
	while(true) {
		scanf("%s", c);
		if(c[0] == '#') break;
		queue<int>ans;
		int flag = -1;
		for(int i = 0; i < cnt; i++) {
			int r = check(s[i], c);
			if(r == 0) { flag = i; break; }
			else if(r == 1)	ans.push(i);
		}
		if(flag != -1) printf("%s is correct\n", c);
		else {
			printf("%s:");
			while(q.size()) {
				printf(" %s", q.top());
				q.pop();
			}
		}
	}
	return 0;
}
